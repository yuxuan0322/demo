<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.cskj.spring.demo.mapper.PowerMapper">
	<select id="queryPowerAll" resultType="power">
		select * from power
	</select>
	
	<insert id="addPower">
		insert into power values(null,#{name},#{country},#{occupation},#{slogan},#{perish})
	</insert>
</mapper>